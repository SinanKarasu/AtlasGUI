 000001 BEGIN, ATLAS PROGRAM 'TRY' $
     70 DECLARE, VARIABLE, 'RECEIVED1','RECEIVED2' IS STRING (20) OF CHAR $
        DEFINE,'CONC',PROCEDURE
           ( 'X' ,'Y' IS STRING (8) OF CHAR) 
            RESULT ('DATA'    IS STRING (20) OF CHAR) $
            CALCULATE, 'DATA' = 'X' & 'Y' $
            LEAVE, 'CONC' $
            OUTPUT,TEXT,FROM 'DATA' $
        END, 'CONC' $
 099000 COMMENCE, MAIN PROCEDURE $
        PERFORM, 'CONC' ,TABLE 3 VAR 2 TIMES
        (C'HELLO',C'THERE') RESULT ('RECEIVED1'), 
        (C'OUCH ',C'COUCH') RESULT ('RECEIVED2') $
        OUTPUT,TEXT,FROM 'RECEIVED1' ,C':','RECEIVED2' $
 999999 TERMINATE, ATLAS PROGRAM 'TRY'        $
