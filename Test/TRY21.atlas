 000001 BEGIN, ATLAS PROGRAM 'TRY' $
     70 DECLARE, VARIABLE, 'X' IS DECIMAL INITIAL =1.1 $
 099000 COMMENCE, MAIN PROCEDURE $
C        CALCULATE, 'X' =1      $
        OUTPUT,TEXT,FROM 'X' $
        FOR , 'X' = 1 THRU 5 , THEN$
           OUTPUT, TEXT, FROM  C'X IS ','X' $
        END, FOR $
        OUTPUT,TEXT,FROM C' FINAL X IS ', 'X' $
 999999 TERMINATE, ATLAS PROGRAM 'TRY'        $
